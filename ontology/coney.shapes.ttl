# baseURI: http://purl.org/coney
# imports: http://datashapes.org/dash
# prefix: coney

@prefix coney: <http://purl.org/coney#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/coney>
  a owl:Ontology ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
coney:AnswerShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Answer shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path [
          sh:inversePath coney:starts ;
        ] ;
      sh:maxCount 0 ;
    ] ;
  sh:targetClass coney:Answer ;
  sh:xone (
      [
        sh:property [
            sh:path coney:leadsTo ;
            sh:class coney:Talk ;
            sh:maxCount 1 ;
          ] ;
      ]
      [
        sh:property [
            sh:path coney:leadsTo ;
            sh:class coney:Question ;
            sh:maxCount 1 ;
          ] ;
      ]
    ) ;
.
coney:ClosedQuestionShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Closed question shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path coney:hasViz ;
      sh:class coney:Viz ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:property [
      sh:path coney:leadsTo ;
      sh:class coney:ClosedAnswer ;
      sh:minCount 1 ;
    ] ;
  sh:targetClass coney:ClosedQuestion ;
.
coney:CompletedQuestionShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Completed question shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path coney:hasAnswer ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:targetClass coney:CompletedQuestion ;
  sh:xone (
      [
        sh:property [
            sh:path coney:completeQuestion ;
            sh:maxCount 0 ;
          ] ;
      ]
      [
        sh:property [
            sh:path (
                coney:hasAnswer
                [
                  sh:inversePath coney:leadsTo ;
                ]
              ) ;
            sh:equals coney:completeQuestion ;
          ] ;
      ]
    ) ;
  sh:xone (
      [
        sh:property [
            sh:path coney:hasAnswerText ;
            sh:maxCount 0 ;
          ] ;
      ]
      [
        sh:property [
            sh:path coney:hasAnswerText ;
            sh:class coney:OpenAnswer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
          ] ;
      ]
    ) ;
.
coney:OpenAnswerShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Open answer shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path coney:hasText ;
      sh:maxCount 0 ;
    ] ;
.
coney:OpenQuestionShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Open question shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path coney:leadsTo ;
      sh:class coney:OpenAnswer ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:targetClass coney:OpenQuestion ;
.
coney:QuestionShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Question shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path coney:leadsTo ;
      sh:class coney:Answer ;
      sh:minCount 1 ;
    ] ;
  sh:targetClass coney:Question ;
.
coney:SurveyShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Survey shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      sh:path coney:starts ;
      sh:class coney:SurveyElement ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:targetClass coney:Survey ;
.
coney:TalkShape
  a rdfs:Class ;
  a sh:NodeShape ;
  rdfs:label "Talk shape" ;
  rdfs:subClassOf rdfs:Resource ;
  sh:targetClass coney:Talk ;
  sh:xone (
      [
        sh:property [
            sh:path coney:leadsTo ;
            sh:class coney:Talk ;
            sh:maxCount 1 ;
          ] ;
      ]
      [
        sh:property [
            sh:path coney:leadsTo ;
            sh:class coney:Question ;
            sh:maxCount 1 ;
          ] ;
      ]
    ) ;
.
