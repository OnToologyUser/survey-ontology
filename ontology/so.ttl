#################################################################

###  https://w3id.org/survey-ontology#hasAnswerText
sur:hasAnswerText rdf:type owl:DatatypeProperty ;
                  rdfs:domain sur:CompletedQuestion ;
                  rdfs:range xsd:string ;
                  rdfs:label "has answer text" .


###  https://w3id.org/survey-ontology#hasCompletionTimestamp
sur:hasCompletionTimestamp rdf:type owl:DatatypeProperty ;
                           rdfs:domain sur:CompletedQuestion ;
                           rdfs:range xsd:dateTime ;
                           rdfs:label "has completion timestamp" .


###  https://w3id.org/survey-ontology#hasLink
sur:hasLink rdf:type owl:DatatypeProperty ;
            rdfs:domain sur:Talk ;
            rdfs:range xsd:anyURI ;
            rdfs:label "has link" .


###  https://w3id.org/survey-ontology#hasOrderNumber
sur:hasOrderNumber rdf:type owl:DatatypeProperty ;
                   rdfs:domain sur:ClosedAnswer ;
                   rdfs:range xsd:integer ;
                   rdfs:label "has order number" .


###  https://w3id.org/survey-ontology#hasPoints
sur:hasPoints rdf:type owl:DatatypeProperty ;
              rdfs:domain sur:ClosedAnswer ;
              rdfs:range xsd:float ;
              rdfs:label "has points" .


###  https://w3id.org/survey-ontology#hasStatus
sur:hasStatus rdf:type owl:DatatypeProperty ;
              rdfs:domain sur:SurveyProcedure ;
              rdfs:range xsd:string ;
              rdfs:label "has status" .


###  https://w3id.org/survey-ontology#hasText
sur:hasText rdf:type owl:DatatypeProperty ;
            rdfs:domain sur:SurveyElement ;
            rdfs:range xsd:string ;
            rdfs:label "has text" .


###  https://w3id.org/survey-ontology#hasTitle
sur:hasTitle rdf:type owl:DatatypeProperty ;
             rdfs:domain sur:SurveyProcedure ;
             rdfs:range xsd:string ;
             rdfs:label "has title" .


###  https://w3id.org/survey-ontology#hasValue
sur:hasValue rdf:type owl:DatatypeProperty ;
             rdfs:domain sur:ClosedAnswer ;
             rdfs:range xsd:float ;
             rdfs:label "has value" .


###  https://w3id.org/survey-ontology#participantId
sur:participantId rdf:type owl:DatatypeProperty ;
                  rdfs:domain sur:Participant ;
                  rdfs:range xsd:string ;
                  rdfs:label "participant id" .


###  https://w3id.org/survey-ontology#sessionId
sur:sessionId rdf:type owl:DatatypeProperty ;
              rdfs:domain sur:SurveyCompletionTask ;
              rdfs:range xsd:string ;
