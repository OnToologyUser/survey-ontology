.
so:hasStatus
  a owl:DatatypeProperty ;
  rdfs:domain so:Survey ;
  rdfs:label "hasStatus" ;
  rdfs:range xsd:string ;
.
so:hasTag
  a owl:ObjectProperty ;
  rdfs:domain so:Question ;
  rdfs:label "hasTag" ;
  rdfs:range so:Tag ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasText
  a owl:DatatypeProperty ;
