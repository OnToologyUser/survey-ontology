so:hasValue
  a owl:DatatypeProperty ;
  rdfs:domain so:ClosedAnswer ;
  rdfs:label "hasValue" ;
  rdfs:range xsd:float ;
.
so:hasViz
  a owl:ObjectProperty ;
  rdfs:domain so:ClosedQuestion ;
  rdfs:label "hasViz" ;
  rdfs:range so:QuestionViz ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:leadsTo
  a owl:ObjectProperty ;
  rdfs:domain so:SurveyElement ;
  rdfs:label "leadsTo" ;
  rdfs:range so:SurveyElement ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:ofSurvey
  a owl:ObjectProperty ;
  rdfs:domain so:SurveyElement ;
  rdfs:label "ofSurvey" ;
  rdfs:range so:Survey ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:starts
  a owl:ObjectProperty ;
  rdfs:domain so:Survey ;
  rdfs:label "starts" ;
  rdfs:range so:SurveyElement ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
ro:Resource
  a owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
wfdesc:Output
  a owl:Class ;
  rdfs:label "Output" ;
  rdfs:subClassOf owl:Thing ;
.
wfdesc:Process
  a owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
wfdesc:hasOutput
  a owl:ObjectProperty ;
  rdfs:domain so:Survey ;
  rdfs:label "hasOutput" ;
