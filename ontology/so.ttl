  rdfs:label "hasAnswerText" ;
  rdfs:range xsd:string ;
.
so:hasOrderNumber
  a owl:DatatypeProperty ;
  rdfs:domain so:ClosedAnswer ;
  rdfs:label "hasOrderNumber" ;
  rdfs:range xsd:integer ;
.
so:hasPoints
  a owl:DatatypeProperty ;
