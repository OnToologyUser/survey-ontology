so:SurveyDataStructureDefinition
  a qb:DataStructureDefinition ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired false ;
      qb:measure so:hasAnswer ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired false ;
      qb:measure so:hasAnswerText ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired true ;
      qb:dimension so:answeredIn ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired true ;
      qb:dimension so:completesQuestion ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired true ;
      qb:measure so:hasCompletionTimestamp ;
    ] ;
  qb:sliceKey so:SliceByQuestion ;
  qb:sliceKey so:SliceBySession ;
  rdfs:label "Survey data structure definition" ;
.
so:SurveyElement
  a owl:Class ;
