# baseURI: http://purl.org/survey-ontology/so#
# prefix: so

@prefix so: <http://purl.org/survey-ontology/so#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ro: <http://purl.org/wf4ever/ro#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix wfdesc: <http://purl.org/wf4ever/wfdesc#> .
@prefix wfprov: <http://purl.org/wf4ever/wfprov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

so:
  a owl:Ontology ;
.

so:Answer
  a owl:Class ;
  rdfs:label "Answer" ;
  rdfs:subClassOf so:SurveyElement ;
.
so:ClosedAnswer
  a owl:Class ;
  rdfs:label "ClosedAnswer" ;
  rdfs:subClassOf so:Answer ;
.
so:ClosedQuestion
  a owl:Class ;
  rdfs:label "ClosedQuestion" ;
  rdfs:subClassOf so:Question ;
.
so:CompletedQuestion
  a owl:Class ;
  rdfs:label "CompletedQuestion" ;
  rdfs:subClassOf wfdesc:Output ;
.
so:CompletedSurvey
  a owl:Class ;
  rdfs:label "CompletedSurvey" ;
  rdfs:subClassOf wfprov:Artifact ;
.
so:Emoji
  a so:QuestionViz ;
  rdfs:label "Emoji" ;
.
so:OpenAnswer
  a owl:Class ;
  rdfs:label "OpenAnswer" ;
  rdfs:subClassOf so:Answer ;
.
so:OpenQuestion
  a owl:Class ;
  rdfs:label "OpenQuestion" ;
  rdfs:subClassOf so:Question ;
.
so:Option
  a so:QuestionViz ;
  rdfs:label "Option" ;
.
so:Participant
  a owl:Class ;
  rdfs:label "Participant" ;
  rdfs:subClassOf prov:Agent ;
.
so:Project
  a owl:Class ;
  rdfs:label "Project" ;
  rdfs:subClassOf so:SurveyTarget ;
.
so:Question
  a owl:Class ;
  rdfs:label "Question" ;
  rdfs:subClassOf so:SurveyElement ;
.
so:QuestionViz
