# baseURI: http://purl.org/survey-ontology/so#
# imports: http://purl.org/linked-data/cube
# imports: http://purl.org/wf4ever/ro#
# imports: http://purl.org/wf4ever/wfdesc
# imports: http://purl.org/wf4ever/wfprov#
# imports: http://w3id.org/rv-ontology
# imports: http://www.w3.org/ns/prov#
# prefix: so

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ro: <http://purl.org/wf4ever/ro#> .
@prefix rvo: <http://w3id.org/rv-ontology#> .
@prefix so: <http://purl.org/survey-ontology/so#> .
@prefix wfdesc: <http://purl.org/wf4ever/wfdesc#> .
@prefix wfprov: <http://purl.org/wf4ever/wfprov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

so:
  a owl:Ontology ;
  owl:imports qb: ;
  owl:imports ro: ;
  owl:imports wfdesc: ;
  owl:imports wfprov: ;
  owl:imports rvo: ;
  owl:imports prov: ;
.
so:Answer
  a owl:Class ;
  rdfs:label "Answer" ;
  rdfs:subClassOf so:SurveyElement ;
  owl:disjointWith so:Question ;
  owl:disjointWith so:Talk ;
.
so:Checkbox
  a so:QuestionVisualization ;
  rdfs:label "Checkbox" ;
.
so:ClosedAnswer
  a owl:Class ;
  rdfs:label "Closed answer" ;
  rdfs:subClassOf so:Answer ;
.
so:ClosedQuestion
  a owl:Class ;
  rdfs:label "Closed question" ;
  rdfs:subClassOf so:Question ;
.
so:CompletedQuestion
  a owl:Class ;
  rdfs:label "Completed question" ;
  rdfs:subClassOf qb:Observation ;
  rdfs:subClassOf wfdesc:Output ;
.
so:CompletedSurvey
  a owl:Class ;
  rdfs:label "CompletedSurvey" ;
  rdfs:subClassOf qb:Slice ;
  rdfs:subClassOf wfprov:Artifact ;
.
so:Emoji
  a so:QuestionVisualization ;
  rdfs:label "Emoji" ;
.
so:LatentVariable
  a owl:Class ;
  rdfs:label "Latent variable" ;
  rdfs:subClassOf rvo:Variable ;
.
so:ObservableVariable
  a owl:Class ;
  rdfs:label "Observable variable" ;
  rdfs:subClassOf rvo:Variable ;
.
so:OpenAnswer
  a owl:Class ;
  rdfs:label "Open answer" ;
  rdfs:subClassOf so:Answer ;
.
so:OpenQuestion
  a owl:Class ;
  rdfs:label "Open question" ;
  rdfs:subClassOf so:Question ;
.
so:Option
  a so:QuestionVisualization ;
  rdfs:label "Option" ;
.
so:Participant
  a owl:Class ;
  rdfs:label "Participant" ;
  rdfs:subClassOf prov:Agent ;
.
so:Question
  a owl:Class ;
  rdfs:label "Question" ;
  rdfs:subClassOf so:SurveyElement ;
  owl:disjointWith so:Answer ;
  owl:disjointWith so:Talk ;
.
so:QuestionVisualization
  a owl:Class ;
  rdfs:label "Question visualization" ;
  rdfs:subClassOf owl:Thing ;
.
so:Scale
  a so:QuestionVisualization ;
  rdfs:label "Scale" ;
.
so:Select
  a so:QuestionVisualization ;
  rdfs:label "Select" ;
.
so:SliceByQuestion
  a qb:SliceKey ;
  qb:componentProperty so:completesQuestion ;
  rdfs:label "Slice by question" ;
.
so:SliceBySession
  a qb:SliceKey ;
  qb:componentProperty so:answeredIn ;
  rdfs:label "Slice by session" ;
.
so:Slider
  a so:QuestionVisualization ;
  rdfs:label "Slider" ;
.
so:Survey
  a owl:Class ;
  rdfs:label "Survey" ;
  rdfs:subClassOf ro:ResearchObject ;
.
so:SurveyCompletion
  a owl:Class ;
  rdfs:label "Survey completion" ;
  rdfs:subClassOf wfprov:ProcessRun ;
.
so:SurveyDataSet
  a owl:Class ;
  rdfs:label "Survey dataset" ;
  rdfs:subClassOf qb:DataSet ;
  rdfs:subClassOf ro:Resource ;
.
so:SurveyDataStructureDefinition
  a qb:DataStructureDefinition ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired false ;
      qb:measure so:hasAnswer ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired false ;
      qb:measure so:hasAnswerText ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired true ;
      qb:dimension so:answeredIn ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired true ;
      qb:dimension so:completesQuestion ;
    ] ;
  qb:component [
      a qb:ComponentSpecification ;
      qb:componentRequired true ;
      qb:measure so:hasCompletionTimestamp ;
    ] ;
  qb:sliceKey so:SliceByQuestion ;
  qb:sliceKey so:SliceBySession ;
  rdfs:label "Survey data structure definition" ;
.
so:SurveyElement
  a owl:Class ;
  rdfs:label "Survey element" ;
  rdfs:subClassOf owl:Thing ;
.
so:SurveyProcedure
  a owl:Class ;
  rdfs:label "Survey procedure" ;
  rdfs:subClassOf ro:Resource ;
  rdfs:subClassOf wfdesc:Process ;
.
so:SurveyTarget
  a owl:Class ;
  rdfs:label "Survey target" ;
  rdfs:subClassOf owl:Thing ;
.
so:Talk
  a owl:Class ;
  rdfs:label "Talk" ;
  rdfs:subClassOf so:SurveyElement ;
  owl:disjointWith so:Answer ;
  owl:disjointWith so:Question ;
.
so:answeredIn
  a owl:ObjectProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "answered in" ;
  rdfs:range so:SurveyCompletion ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:completedFor
  a owl:ObjectProperty ;
  rdfs:domain so:SurveyCompletion ;
  rdfs:label "completed for" ;
  rdfs:range so:SurveyTarget ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:completesQuestion
  a owl:ObjectProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "completes question" ;
  rdfs:range so:Question ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasAnswer
  a owl:ObjectProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "has answer" ;
  rdfs:range so:Answer ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasAnswerText
  a owl:DatatypeProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "has answer text" ;
  rdfs:range xsd:string ;
.
so:hasCompletionTimestamp
  a owl:DatatypeProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "has completion timestamp" ;
  rdfs:range xsd:dateTime ;
.
so:hasLatentVariable
  a owl:ObjectProperty ;
  rdfs:domain so:Question ;
  rdfs:label "has latent variable" ;
  rdfs:range so:LatentVariable ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasLink
  a owl:DatatypeProperty ;
  rdfs:domain so:Talk ;
  rdfs:label "has link" ;
  rdfs:range xsd:anyURI ;
.
so:hasObservableVariable
  a owl:ObjectProperty ;
  rdfs:domain so:Question ;
  rdfs:label "has observable variable" ;
  rdfs:range so:ObservableVariable ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasOrderNumber
  a owl:DatatypeProperty ;
  rdfs:domain so:ClosedAnswer ;
  rdfs:label "has order number" ;
  rdfs:range xsd:integer ;
.
so:hasPoints
  a owl:DatatypeProperty ;
  rdfs:domain so:ClosedAnswer ;
  rdfs:label "has points" ;
  rdfs:range xsd:float ;
.
so:hasStatus
  a owl:DatatypeProperty ;
  rdfs:domain so:SurveyProcedure ;
  rdfs:label "has status" ;
  rdfs:range xsd:string ;
.
so:hasText
  a owl:DatatypeProperty ;
  rdfs:domain so:SurveyElement ;
  rdfs:label "has text" ;
  rdfs:range xsd:string ;
.
so:hasTitle
  a owl:DatatypeProperty ;
  rdfs:domain so:SurveyProcedure ;
  rdfs:label "has title" ;
  rdfs:range xsd:string ;
.
so:hasValue
  a owl:DatatypeProperty ;
  rdfs:domain so:ClosedAnswer ;
  rdfs:label "has value" ;
  rdfs:range xsd:float ;
.
so:hasVisualization
  a owl:ObjectProperty ;
  rdfs:domain so:ClosedQuestion ;
  rdfs:label "has visualization" ;
  rdfs:range so:QuestionVisualization ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:inSurveyProcedure
  a owl:ObjectProperty ;
  rdfs:domain so:SurveyElement ;
  rdfs:label "in survey procedure" ;
  rdfs:range so:SurveyProcedure ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:leadsTo
  a owl:ObjectProperty ;
  rdfs:domain so:SurveyElement ;
  rdfs:label "leads to" ;
  rdfs:range so:SurveyElement ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:participantId
  a owl:DatatypeProperty ;
  rdfs:domain so:Participant ;
  rdfs:label "participant id" ;
  rdfs:range xsd:string ;
.
so:sessionId
  a owl:DatatypeProperty ;
  rdfs:domain so:SurveyCompletion ;
  rdfs:label "session id" ;
  rdfs:range xsd:string ;
.
so:startsWith
  a owl:ObjectProperty ;
  rdfs:domain so:SurveyProcedure ;
  rdfs:label "starts with" ;
  rdfs:range so:SurveyElement ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
