#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/survey-ontology#answeredIn
sur:answeredIn rdf:type owl:ObjectProperty ;
               rdfs:domain sur:CompletedQuestion ;
               rdfs:range sur:SurveyCompletionTask ;
               rdfs:label "answered in" .


###  https://w3id.org/survey-ontology#completedFor
sur:completedFor rdf:type owl:ObjectProperty ;
                 rdfs:domain sur:SurveyCompletionTask ;
                 rdfs:range sur:SurveyTarget ;
                 rdfs:label "completed for" .


###  https://w3id.org/survey-ontology#completesQuestion
sur:completesQuestion rdf:type owl:ObjectProperty ;
                      rdfs:domain sur:CompletedQuestion ;
                      rdfs:range sur:Question ;
                      rdfs:label "completes question" .


###  https://w3id.org/survey-ontology#hasAnswer
sur:hasAnswer rdf:type owl:ObjectProperty ;
              rdfs:domain sur:CompletedQuestion ;
              rdfs:range sur:Answer ;
              rdfs:label "has answer" .


###  https://w3id.org/survey-ontology#hasLatentVariable
sur:hasLatentVariable rdf:type owl:ObjectProperty ;
                      rdfs:domain sur:Question ;
                      rdfs:range sur:LatentVariable ;
                      rdfs:label "has latent variable" .


###  https://w3id.org/survey-ontology#hasObservableVariable
sur:hasObservableVariable rdf:type owl:ObjectProperty ;
                          rdfs:domain sur:Question ;
                          rdfs:range sur:ObservableVariable ;
                          rdfs:label "has observable variable" .


###  https://w3id.org/survey-ontology#hasVisualization
sur:hasVisualization rdf:type owl:ObjectProperty ;
                     rdfs:domain sur:MultipleChoiceQuestion ;
                     rdfs:range sur:QuestionVisualization ;
                     rdfs:label "has visualization" .


###  https://w3id.org/survey-ontology#inSurveyProcedure
sur:inSurveyProcedure rdf:type owl:ObjectProperty ;
                      rdfs:domain sur:SurveyElement ;
                      rdfs:range sur:SurveyProcedure ;
                      rdfs:label "in survey procedure" .


###  https://w3id.org/survey-ontology#leadsTo
sur:leadsTo rdf:type owl:ObjectProperty ;
            rdfs:domain sur:SurveyElement ;
            rdfs:range sur:SurveyElement ;
            rdfs:label "leads to" .


###  https://w3id.org/survey-ontology#startsWith
sur:startsWith rdf:type owl:ObjectProperty ;
               rdfs:domain sur:SurveyProcedure ;
               rdfs:range sur:SurveyElement ;
               rdfs:label "starts with" .


