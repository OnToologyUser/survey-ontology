  rdfs:label "SurveyTarget" ;
  rdfs:subClassOf owl:Thing ;
.
so:Tag
  a owl:Class ;
  rdfs:label "Tag" ;
  rdfs:subClassOf owl:Thing ;
.
so:Talk
  a owl:Class ;
  rdfs:label "Talk" ;
  rdfs:subClassOf so:SurveyElement ;
.
so:completeQuestion
  a owl:ObjectProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "completeQuestion" ;
  rdfs:range so:Question ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:completedFor
  a owl:ObjectProperty ;
  rdfs:domain so:CompletedSurvey ;
  rdfs:label "completedFor" ;
  rdfs:range so:SurveyTarget ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasAnswer
  a owl:ObjectProperty ;
  rdfs:domain so:CompletedQuestion ;
  rdfs:label "hasAnswer" ;
  rdfs:range so:Answer ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
so:hasAnswerText
  a owl:DatatypeProperty ;
