.
so:Survey
  a owl:Class ;
  rdfs:subClassOf ro:Resource ;
  rdfs:subClassOf wfdesc:Process ;
.
so:SurveyCompletion
  a owl:Class ;
  rdfs:label "SurveyCompletion" ;
  rdfs:subClassOf wfprov:ProcessRun ;
.
so:SurveyElement
  a owl:Class ;
  rdfs:label "Survey element" ;
  rdfs:subClassOf prov:Entity ;
.
so:SurveyTarget
