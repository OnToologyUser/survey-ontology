  rdfs:label "endedAtTime" ;
  rdfs:range xsd:dateTime ;
.
prov:startedAtTime
  a owl:DatatypeProperty ;
  rdfs:domain prov:Activity ;
  rdfs:label "startedAtTime" ;
  rdfs:range xsd:dateTime ;
